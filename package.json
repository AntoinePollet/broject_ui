{
  "name": "broject_ui",
  "version": "1.0.4",
  "type": "module",
  "types": "./dist/index.d.ts",
  "description": "Simple UI library based on headlessUI for vuejs",
  "repository": {
    "url": "https://github.com/AntoinePollet/broject_ui"
  },
  "files": [
    "dist"
  ],
  "index": "./dist/broject_ui.umd.cjs",
  "module": "./dist/broject_ui.js",
  "exports": {
    ".": {
      "import": "./dist/broject_ui.js",
      "require": "./dist/broject_ui.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build && npm run compile-css && npm run merge-css",
    "compile-css": "npx tailwindcss -i ./lib/index.css -o ./lib/build.css && npx tailwindcss -o ./lib/build.css --minify",
    "merge-css": "node mergeCss.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@popperjs/core": "^2.11.8",
    "postcss": "^8.4.31",
    "vite-plugin-dts": "^3.6.3",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.16",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0"
  }
}
